{
  "name": "naver-shop-search",
  "version": "1.0.0",
  "description": "네이버 쇼핑 검색 자동화 (Playwright 모바일 에뮬레이션)",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:vpn": "sudo ./vpn/run-in-vpn.sh 18 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=18",
    "start:vpn:16": "sudo ./vpn/run-in-vpn.sh 16 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=16",
    "start:vpn:17": "sudo ./vpn/run-in-vpn.sh 17 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=17",
    "start:vpn:18": "sudo ./vpn/run-in-vpn.sh 18 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=18",
    "start:vpn:19": "sudo ./vpn/run-in-vpn.sh 19 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=19",
    "start:vpn:20": "sudo ./vpn/run-in-vpn.sh 20 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=20",
    "start:vpn:21": "sudo ./vpn/run-in-vpn.sh 21 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=21",
    "start:vpn:22": "sudo ./vpn/run-in-vpn.sh 22 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=22",
    "start:vpn:23": "sudo ./vpn/run-in-vpn.sh 23 /home/tech/naver/shop/node_modules/.bin/ts-node src/index.ts --vpn=23",
    "search": "ts-node src/index.ts --search-only",
    "repeat": "ts-node src/index.ts --repeat",
    "repeat:3": "ts-node src/index.ts --repeat=3",
    "repeat:5": "ts-node src/index.ts --repeat=5",
    "repeat:10": "ts-node src/index.ts --repeat=10",
    "repeat:20": "ts-node src/index.ts --repeat=20",
    "repeat:50": "ts-node src/index.ts --repeat=50",
    "repeat:100": "ts-node src/index.ts --repeat=100",
    "repeat:forever": "ts-node src/index.ts --repeat=0",
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts",
    "vpn:up": "sudo ./vpn/vpn-up.sh 16",
    "vpn:16": "sudo ./vpn/vpn-up.sh 16",
    "vpn:17": "sudo ./vpn/vpn-up.sh 17",
    "vpn:18": "sudo ./vpn/vpn-up.sh 18",
    "vpn:19": "sudo ./vpn/vpn-up.sh 19",
    "vpn:20": "sudo ./vpn/vpn-up.sh 20",
    "vpn:21": "sudo ./vpn/vpn-up.sh 21",
    "vpn:22": "sudo ./vpn/vpn-up.sh 22",
    "vpn:23": "sudo ./vpn/vpn-up.sh 23",
    "vpn:down": "sudo ./vpn/vpn-down.sh",
    "vpn:down:16": "sudo ./vpn/vpn-down.sh 16",
    "vpn:down:17": "sudo ./vpn/vpn-down.sh 17",
    "vpn:down:18": "sudo ./vpn/vpn-down.sh 18",
    "vpn:down:19": "sudo ./vpn/vpn-down.sh 19",
    "vpn:down:20": "sudo ./vpn/vpn-down.sh 20",
    "vpn:down:21": "sudo ./vpn/vpn-down.sh 21",
    "vpn:down:22": "sudo ./vpn/vpn-down.sh 22",
    "vpn:down:23": "sudo ./vpn/vpn-down.sh 23",
    "vpn:start": "sudo ./vpn/run-in-vpn.sh npm start",
    "parallel": "sudo ./vpn/run-parallel.sh",
    "parallel:5": "sudo ./vpn/run-parallel.sh 5",
    "parallel:10": "sudo ./vpn/run-parallel.sh 10",
    "parallel:forever": "sudo ./vpn/run-parallel.sh forever"
  },
  "dependencies": {
    "mysql2": "^3.15.3",
    "patchright": "^1.57.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  }
}
